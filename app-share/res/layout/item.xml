<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:padding="2dip"
	android:layout_height="86dip"
	android:layout_width="76dip"
	android:orientation="vertical"
	android:gravity="center_vertical"
	>
<TextView
	android:id="@+id/label1"
	android:textSize="12dip"
	android:textColor="#ffffffff"
	android:gravity="center_horizontal|center_vertical"
	android:singleLine="true" android:layout_width="fill_parent" android:layout_height="wrap_content" android:maxLines="1" android:minLines="1" android:ellipsize="none"/>
<TextView
	android:id="@+id/label2"
	android:textSize="12dip"
	android:textColor="#ffffffff"
	android:gravity="center_horizontal|center_vertical"
	android:singleLine="true" android:layout_width="fill_parent" android:layout_height="wrap_content" android:maxLines="1" android:minLines="1" android:ellipsize="start"/>
		
</LinearLayout>